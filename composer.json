  "support": {
    "email": "support@rmsoft.be",
    "source": "https://github.com/RudyMas/easymvc"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "rudymas/pdo-ext": "*",
    "rudymas/router": "*",
    "rudymas/html5": "*",
    "rudymas/xml_json": "*",
    "rudymas/manipulator": "*",
    "twig/twig": "*",
    "guzzlehttp/guzzle": "*",
    "nette/mail": "^2",
    "latte/latte": "^2"
