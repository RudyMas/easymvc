{
  "name": "easymvc/easymvc",
  "description": "EasyMVC PHP Framework with Angular 5 support",
  "type": "project",
  "version": "0.9.0",
  "keywords": [
    "MVC",
    "Framework",
    "PHP",
    "Angular"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rudy Mas",
      "email": "rudy.mas@rmssoft.be",
      "homepage": "http://www.rmsoft.be",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@rmsoft.be",
    "source": "https://github.com/RudyMas/easymvc"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "rudymas/router": "*",
    "easymvc/controller": "*",
    "easymvc/repository": "*",
    "easymvc/email": "*",
    "easymvc/httprequest": "*",
    "easymvc/login": "*",
    "easymvc/menu": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Model\\": "src/models",
      "Repository\\": "src/repositories",
      "Controller\\": "src/controllers",
      "View\\": "src/views",
      "Test\\": "tests"
    }
  }
}