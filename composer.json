{
  "name": "rudymas/easymvc",
  "description": "Easy MVC PHP Framework",
  "type": "project",
  "version": "0.7.0",
  "keywords": [
    "MVC",
    "Framework",
    "PHP"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rudy Mas",
      "email": "rudy.mas@rmssoft.be",
      "homepage": "http://www.rmsoft.be",
      "role": "Developer"
    }
